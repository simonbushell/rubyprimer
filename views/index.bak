  <body>
    <div class="navbar navbar-fixed-top navbar-inverse">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="#">
            RubyPrimer
          </a>
          <ul class="nav">
            <li>
              <a href="#">
                Home
              </a>
            </li>
            <li>
              <a href="#">
                About
              </a>
            </li>
            <li>
              <a href="#">
                Contact
              </a>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <div class="btn-group">
    </div>
    <div class="container">
      <div>
        <p>
          An automated system for designing oligonucleotide primers to effect protein
          truncations and deletions
        </p>
      </div>
      <div class="row">
        <div class="span6">
          <div class="control-group">
          </div>
          <div>
            <p>
              DNA Coding Sequence
            </p>
          </div>
          <textarea id="dnainput" name="DNAinput"></textarea>
          <div>
            <p>
              Translation
            </p>
          </div>
          <div class="well">
            <div id="DNAtranslation">
              <p>
                
              </p>
            </div>
          </div>
        </div>
        <div class="span6">
          <div class="control-group">
          </div>
          <div class="control-group">
          </div>
          <div>
            <p>
              Truncation Strings
            </p>
          </div>
          <div class="control-group">
            <textarea id="experimentInput" name="textarea3"></textarea>
          </div>
          <div class="control-group">
            <label for="textinput1">
              Initial Primer-Primer Tm
            </label>
            <input name="textinput1" type="" placeholder="50">
          </div>
          <div class="control-group">
            <label for="textinput2">
              Initial Primer-Template Tm
            </label>
            <input name="textinput2" type="" placeholder="45">
          </div>
          <div class="control-group">
          </div>
          <div class="control-group">
            <label for="selectinput1">
              Force G-C End?
            </label>
            <select name="selectinput1">
            </select>
          </div>
          <a id="submitButton" class="btn btn-primary" href="#">
            Submit
          </a>
        </div>
      </div>
    </div>

    <style>
      textarea {
          -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
          -moz-box-sizing: border-box;    /* Firefox, other Gecko */
          box-sizing: border-box;         /* Opera/IE 8+ */
      }
      
      #dnainput {
          font-family:Courier New, monospace;
          height:240px;
          width:100%;
      }
      
      #experimentInput {
          font-family:Courier New, monospace;
          height:240px;
      }
      
      #translateDNA, #clearDNA {
          float:right;
          margin-bottom:5px;
      }
      
      #DNAtranslation p{
          color: red;
          font-family: Courier New, monospace;
          word-wrap:break-word;
      }
      
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js">
    </script>
    <script src="assets/js/bootstrap.js">
