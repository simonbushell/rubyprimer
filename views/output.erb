<% for x in @results %>
<div class="outputContainer">
        <div class="row">
            <div class="span10 offset1">
                <h3><%= x.experimentString.upcase %></h3>
                <div class="row">
                    <div class="span8">
                        <% if x.class == ErrorExperiment %>
                            <p class="experimentErrorText">Error: <%= x.errorString %></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
                        <% else %>
                            <div class="row">
                                <div class="span2">
                                    Forward Primer:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.printForwardPrimer %> (<%= x.forwardPrimer.length %> bp)</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Reverse Primer:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.printReversePrimer %> (<%= x.reversePrimer.length %> bp)<span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Primer-Primer Tm:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.ppTM %></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Fwd Primer-Template Tm:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.forwardPrimerTemplate.tm.round(1) %></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Rev Primer-Template Tm:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.reversePrimerTemplate.tm.round(1) %></span>
                                </div>
                            </div>
                        </div>
                        <div class="span2">
                            <input type="button" class="btn btn-success btn-small showCyclingParameters" value="Cycling Parameters"></input>
                            <input type="button" class="btn btn-info btn-small showMutatedTemplate" value="Show Mutated Template"></input>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="span10">
                    <div class="span5 well mutatedTemplate">
                        <p><%= x.mutatedTemplate %></p>
                    </div>
                    <div class="span4 well cyclingParameters">
                        <p>Assuming <em>pfu</em> polymerase</p>
                        <ul>
                            <li>95C for 7 minutes</li>
                            <li><%= x.pcrConditions[:cycles] %> cycles of</li>
                            <ul>
                                <li>95C for 1 minute</li>
                                <li><%= x.pcrConditions[:annealing] %>C for 1 minute</li>
                                <li>68C for 1 minute per 1kB plasmid length</li>
                            </ul>
                            <li><%= x.pcrConditions[:finalannealing]%>C for 5 minutes</li>
                            <li>68C for 15 minutes</li>
                            <li>Hold at 4C</li>
                        </ul>
                    </div>
                </div>
            </div>
    </div>
    <% end %>
    <hr>
<% end %>