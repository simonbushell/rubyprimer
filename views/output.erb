<% for x in @results %>
<div class="outputContainer">
        <div class="row">
            <div class="span10 offset1">
                <h3><%= x.experimentString.upcase %></h3>
                <div class="row">
                    <div class="span8">
                        <% if x.class == ErrorExperiment %>
                            <p class="experimentErrorText">Error: <%= x.errorString %></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
                        <% else %>
                            <div class="row">
                                <div class="span2">
                                    Forward Primer:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.printForwardPrimer %> (<%= x.forwardPrimer.length %> bp)</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Reverse Primer:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.printReversePrimer %> (<%= x.reversePrimer.length %> bp)<span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Primer-Primer Tm:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.ppTM %></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Fwd Primer-Template Tm:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.forwardPrimerTemplate.tm.round(1) %></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="span8">
                            <div class="row">
                                <div class="span2">
                                    Rev Primer-Template Tm:
                                </div>
                                <div class="span6">
                                    <span class="primerOutput"><%= x.reversePrimerTemplate.tm.round(1) %></span>
                                </div>
                            </div>
                        </div>
                        <div class="span2">
                            <input type="button" class="btn btn-info btn-small showMutatedTemplate" value="Show Mutated Template"></input>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="span8 offset1 well mutatedTemplate">
                        <p><%= x.mutatedTemplate %></p>
                </div>
            </div>
    </div>
    <% end %>
    <hr>
<% end %>