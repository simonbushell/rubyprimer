<% for x in @results %>
        <div class="row">
            <div class="span10 offset1">
                <h3><%= x.experimentString.upcase %></h3>
                <div class="row">
                    <div class="span8">
                        <div class="row">
                            <div class="span2">
                                Forward Primer:
                            </div>
                            <div class="span6">
                                <span class="primerOutput"><%= x.printForwardPrimer %></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="span8">
                        <div class="row">
                            <div class="span2">
                                Reverse Primer:
                            </div>
                            <div class="span6">
                                <span class="primerOutput"><%= x.printReversePrimer %> (<%= x.reversePrimer.length %>bp)<span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="span8">
                        <div class="row">
                            <div class="span2">
                                Primer-Primer Tm:
                            </div>
                            <div class="span6">
                                <span class="primerOutput"><%= x.ppTM %></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="span8">
                        <div class="row">
                            <div class="span2">
                                Fwd Primer-Template Tm:
                            </div>
                            <div class="span6">
                                <span class="primerOutput"><%= x.forwardPrimerTemplate.tm.round(1) %></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="span8">
                        <div class="row">
                            <div class="span2">
                                Rev Primer-Template Tm:
                            </div>
                            <div class="span6">
                                <span class="primerOutput"><%= x.reversePrimerTemplate.tm.round(1) %></span>
                            </div>
                        </div>
                    </div>
                </div>
                <hr>
            </div>
        </div>
<% end %>